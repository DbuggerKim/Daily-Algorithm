-- 코드를 입력하세요
SELECT MCDP_CD AS 진료과코드, COUNT(APNT_YMD) AS 5월예약건수
FROM APPOINTMENT
WHERE APNT_YMD LIKE '2022-05%'
GROUP BY MCDP_CD
ORDER BY 2,1


-- 코드를 입력하세요 OLD
SELECT MCDP_CD AS '진료과코드', COUNT(MCDP_CD) AS '5월예약건수'
FROM APPOINTMENT
WHERE YEAR(APNT_YMD)=2022 AND MONTH(APNT_YMD)=05
GROUP BY MCDP_CD
-- HAVING COUNT(PT_NO)
ORDER BY PT_NO ASC, MCDP_CD ASC
