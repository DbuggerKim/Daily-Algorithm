// you can also use imports, for example:
import java.util.*;


class Solution {
    public int solution(int K, int[] A) {

        int count=0;
        int sum=0;

        for(int i : A){
            sum+=i;
            if(sum>=K){
                count++;
                sum=0;
            }
        }

        return count;

    }
}

//100%
