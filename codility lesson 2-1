
class Solution {
    public int[] solution(int[] A, int K) {

        if(A.length==1 || A.length==0) return A;

        else{
            for(int j=0; j<K; j++){
                int temp=A[A.length-1];
                for(int i=A.length-2; i>=0 ; i--)
                    A[i+1]=A[i];
                A[0]=temp;
            }
            return A;
        }
    }
}
